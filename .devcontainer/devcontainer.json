{
  "name": "viberpg",
  "dockerComposeFile": "docker-compose.yml",
  "service": "app",
  "workspaceFolder": "/workspaces/viberpg",
  "runServices": ["mongo"],
  "forwardPorts": [3000, 27017],
  "shutdownAction": "stopCompose",
  "remoteUser": "vscode",
  "postCreateCommand": "cargo fetch",
  "customizations": {
    "vscode": {
      "extensions": [
        "rust-lang.rust-analyzer"
      ]
    }
  }
}
